{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/node": "^12.7.2",
    "@types/node-fetch": "^2.5.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "watch": "nodemon",
    "start": "node --inspect=5858 -r ts-node/register ./index.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "."
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
